{
  "publisher": "mwpb",
  "name": "java-prettier-formatter",
  "displayName": "Java prettier formatter",
  "description": "Formats Java using the Prettier formatter.",
  "version": "0.1.1",
  "engines": {
    "vscode": "^1.45.0"
  },
  "categories": [
    "Formatters"
  ],
  "keywords": [
    "prettier",
    "Java",
    "formatter"
  ],
  "activationEvents": [
    "onLanguage:java"
  ],
  "main": "./extension.js",
  "preview": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/mwpb/java-prettier-formatter"
  },
  "icon": "prettier-avatar-dark.png",
  "contributes": {
    "languages": [
      {
        "id": "java",
        "aliases": [
          "Java"
        ],
        "extensions": [
          "java"
        ]
      }
    ]
  },
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "vscode": "^1.1.37"
  },
  "dependencies": {
    "prettier": "^2.0.5",
    "prettier-plugin-java": "^0.8.0"
  }
}
